<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="test_track">
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Simple obstacle -->
    <model name="box_obstacle">
      <pose>1 0 0.25 0 0 0</pose>
      <link name="box_link">
        <visual name="vis">
          <geometry>
            <box>
              <size>0.4 0.4 0.5</size>
            </box>
          </geometry>
        </visual>
        <collision name="col">
          <geometry>
            <box>
              <size>0.4 0.4 0.5</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>

    <!-- Stop sign model from Gazebo Fuel (download first run) -->
    <include>
      <uri>https://fuel.gazebosim.org/1.0/OpenRobotics/models/Stop%20Sign</uri>
      <name>stop_sign</name>
      <pose>3 0 0 0 0 1.57</pose>
    </include>

  </world>
</sdf> 